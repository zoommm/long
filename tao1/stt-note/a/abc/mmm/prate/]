#include <stdio.h>
void print_array(int p[],int n);
void init_array(int p[], int n);

int main(int argc, const char *argv[])
{
    int array[100];
    printf("%d\n",array);
    init_array(array,100);
    print_array(array,100);
    return 0;
}
void init_array(int p[], int n)

{
    int i=0;
    for (i = 0; i < n; i++) 
    {
        p[i]=i+1;
    }
}
void print_array(int p[],int n)
{
    int i=0;
    for (i = 0; i < n; i++) 
    {
        printf("%5d",p[i]);
    }
    printf("%d\n");
}
